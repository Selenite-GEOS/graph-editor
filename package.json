		"svelte-css-vars": "^0.0.3",
		"svelte-fa": "^4.0.2",
		"svelte-i18n": "^4.0.0",
		"svelte-persisted-store": "^0.11.0",
		"svelte-portal": "^2.2.1",
		"uuid": "^10.0.0",
		"wu": "^2.1.0"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix",
		"*.{js,css,md}": "prettier --write"
	}
}
